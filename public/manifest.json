{
  "manifest_version": 3,
  "name": "Daily Notes",
  "version": "1.0.0",
  "description": "A beautiful note-taking app for your new tab page",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
